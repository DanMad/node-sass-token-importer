{
  "name": "node-sass-token-importer",
  "version": "0.0.0-development",
  "description": "Allows the importing of TypeScript, JavaScript, and JSON in Sass files parsed by node-sass",
  "keywords": [
    "importer",
    "javascript",
    "json",
    "json5",
    "node-sass",
    "sass",
    "typescript",
    "tokens"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/node-sass-token-importer.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "mkdirp dist && babel src -d dist",
    "prepublishOnly": "npm run build",
    "sort-package": "sort-package-json",
    "test": "mocha --require ./utils/babel-register.js",
    "prepare": "husky install",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@babel/register": "^7.13.16",
    "is-there": "^4.5.1",
    "json5": "^2.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "chai": "^4.3.4",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mkdirp": "^1.0.4",
    "mocha": "^8.3.2",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "sort-package-json": "^1.49.0"
  },
  "peerDependencies": {
    "node-sass": ">=5.0.0"
  },
  "engines": {
    "node": "12.19.0"
  }
}
